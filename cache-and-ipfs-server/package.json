    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "apollo-server-express": "^3.10.0",
    "apollo-server-plugin-base": "^3.6.2",
